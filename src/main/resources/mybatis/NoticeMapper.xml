<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.fastlms_project.admin.notice.mapper.NoticeMapper">

    <select id="noticeList" resultType="com.example.fastlms_project.admin.notice.dto.NoticeDto">
        select *
        from notice
        where 1 = 1
    </select>

    <select id="noticeCount" resultType="long">
        select count(*)
        from notice
        where 1 = 1
    </select>

</mapper>